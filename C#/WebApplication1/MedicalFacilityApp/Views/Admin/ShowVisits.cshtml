@model IEnumerable<Visit>


@foreach (var visit in Model)
{
    <div>Doctors name: @visit.Doctor.Name</div>
    <div>SurName: @visit.Doctor.SurName</div>
    <div>Specialty: @visit.Doctor.Specialty</div>
    <div>Patients name: @visit.Patient.Name</div>
    <div>Patients Surname: @visit.Patient.SurName</div>
    <div>Reason of the visit: @visit.ReasonOfVisit</div>
    
}
